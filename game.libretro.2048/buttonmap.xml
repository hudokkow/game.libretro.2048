<?xml version="1.0" encoding="UTF-8"?>
<buttonmap>
	<controller id="game.controller.default" type="joypad">
		<!--
			The default controller is handled by game.libretro automatically and
			doesn't need a button map. Overridding the button map here informs
			game.libretro that it should ignore features not listed in this button map.
		-->
		<feature name="start" mapto="start"/>
		<feature name="back" mapto="back">
		<feature name="up" mapto="up">
		<feature name="down" mapto="down">
		<feature name="right" mapto="right">
		<feature name="left" mapto="left"/>
	</controller>
</buttonmap>
